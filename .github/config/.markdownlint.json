{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/refs/heads/main/schema/markdownlint-config-schema.json",
  "ul-indent":false,
  "line-length": false,
  "no-inline-html": {
    "allowed_elements": [
      "img",
      "svg",
      "ul",
      "li",
      "text",
      "path",
      "g",
      "country",
      "area",
      "dcnumber",
      "u","user",
      "password",
      "url",
      "email",
      "nickname",
      "description",
      "role",
      "field",
      "value",
      "yyyy-mm-dd-hh-ii-ss",
      "brand",
      "sup",
      "atx",
      "name",
      "zone",
      "destination",
      "project-name",
      "container",
      "tag",
      "KEY",
      "br",
      "tenant"

    ]
  },
  "code-block-style": false,
  "fenced-code-language": false,
  "no-alt-text": false,
  "no-emphasis-as-heading": false,
  "list-indent": false,
  "heading-increment": false,
  "no-duplicate-heading": false,
  "first-line-heading": false,
  "heading-style": {
    "style": "atx"
  },
  "ol-prefix": false,
  "single-h1": false,
  "single-title": false
}
